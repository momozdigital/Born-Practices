position: relative;
padding: 0;
overflow: hidden;
opacity: 0;
visibility: hidden;
height: 0;
transition: all 0.25s ease-out;
-webkit-transition: all 0.25s ease-out;
-moz-transition: all 0.25s ease-out;
-ms-transition: all 0.25s ease-out;
}

#submenu.show {
opacity: 1;
visibility: visible;
height: auto;
padding: 20px 0 40px;
}

#submenu .place {
margin: 0;
