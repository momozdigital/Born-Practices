#minicart {
position: fixed;
top: 0;
right: -100vw;
width: 350px;
max-width: 400px;
height: 100vh;
background: #f2eee6;
padding: 50px 20px 118px;
z-index: 2000;
box-shadow: 1px 1px 18px 1px rgba(0, 0, 0, 0.2);
overflow: hidden;
opacity: 0;
transition: all 0.5s ease-out;
-webkit-transition: all 0.5s ease-out;
-moz-transition: all 0.5s ease-out;
-ms-transition: all 0.5s ease-out;
}

#minicart .top {
position: fixed;
padding: 20px 20px 5px;
background: #f2eee6;
width: 350px;
max-width: 400px;
top: 0;
right: -100vw;
transition: all 0.5s ease-out;
-webkit-transition: all 0.5s ease-out;
-moz-transition: all 0.5s ease-out;
-ms-transition: all 0.5s ease-out;
}

#minicart .top:after {
content: '';
display: block;
width: calc(100% - 40px);
height: 2px;
background: #000;
position: absolute;
left: 20px;
bottom: 0;
}

#minicart .top h2 {
font-size: 2em;
text-transform: uppercase;
font-weight: 700;
text-align: center;
}

#minicart .top .cerrar {
position: absolute;
top: 10px;
transform: rotate(45deg);
right: 30px;
font-size: 1.75em;
color: #000;
cursor: pointer;
}

#minicart .top .cerrar:hover {
opacity: 0.8;
  text-decoration: none;
}

#minicart .middle {
  overflow: hidden;
  /* overflow-x: hidden; */
}

#minicart .middle ul li {
width: 100%;
overflow: hidden;
display: flex;
  flex-wrap: wrap;
margin: 10px 0 10px;
  padding-bottom: 10px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.4);
}

#minicart .middle ul li .left {
width: calc(100% - 120px);
float: left;
padding-right: 20px;
  order: 2;
}

